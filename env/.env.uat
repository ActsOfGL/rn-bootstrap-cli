# UAT Environment Configuration
NODE_ENV=staging
API_BASE_URL=https://api-uat.example.com
GRAPHQL_ENDPOINT=https://graphql-uat.example.com/graphql

# Firebase Configuration
FIREBASE_API_KEY=your_firebase_api_key_uat
FIREBASE_AUTH_DOMAIN=your-project-uat.firebaseapp.com
FIREBASE_PROJECT_ID=your-project-uat
