    "reactotron-redux": "^3.1.9",
    "reactotron-flipper": "^2.0.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "^3.1.1",
    "react-test-renderer": "19.1.0",
    "typescript": "5.0.4",
