    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "react-test-renderer": "18.3.1",
    "typescript": "5.6.3",
    "detox": "^20.27.3",
    "@types/react-native-sqlite-storage": "^6.0.5",
    "@types/react-native-vector-icons": "^6.4.18"
