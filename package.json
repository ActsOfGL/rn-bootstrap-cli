{
  "name": "rn-bootstrap-cli",
  "version": "1.0.0",
  "description": "A comprehensive React Native CLI bootstrap tool with modern features and best practices",
  "main": "cli.js",
  "bin": {
    "rn-bootstrap": "./cli.js",
    "rn-bootstrap-cli": "./cli.js"
  },
  "keywords": [
    "react-native",
    "cli",
    "bootstrap",
    "template",
    "typescript",
    "navigation",
    "state-management",
    "offline-first"
  ],
  "author": "ActsOfGL",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ActsOfGL/rn-bootstrap-cli.git"
  },
  "bugs": {
    "url": "https://github.com/ActsOfGL/rn-bootstrap-cli/issues"
  },
  "homepage": "https://github.com/ActsOfGL/rn-bootstrap-cli#readme",
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "clean": "react-native clean",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "clean:ios": "cd ios && xcodebuild clean && cd ..",
    "pod-install": "cd ios && pod install && cd ..",
    "reactotron": "reactotron-cli",
    "storybook": "storybook dev -p 7007",
    "build-storybook": "storybook build",
    "verify": "node scripts/verify-setup.js",
    "cli:test": "node cli.js",
    "prepublishOnly": "npm run verify"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-native": "0.76.3",
