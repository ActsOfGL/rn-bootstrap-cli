# Default Environment Configuration Template
NODE_ENV=development
API_BASE_URL=https://api-dev.example.com
GRAPHQL_ENDPOINT=https://graphql-dev.example.com/graphql

# Firebase Configuration
FIREBASE_API_KEY=your_firebase_api_key
FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
FIREBASE_PROJECT_ID=your-project-id
FIREBASE_STORAGE_BUCKET=your-project.appspot.com
FIREBASE_MESSAGING_SENDER_ID=123456789
FIREBASE_APP_ID=1:123456789:android:abcdef123456

# Mapbox Configuration
MAPBOX_ACCESS_TOKEN=pk.your_mapbox_access_token

# Sentry Configuration
SENTRY_DSN=https://your_sentry_dsn@sentry.io/project_id

# Other API Keys
ANALYTICS_API_KEY=your_analytics_api_key
PUSH_NOTIFICATION_KEY=your_push_notification_key

# Database Configuration
DATABASE_NAME=app_database.db
DATABASE_VERSION=1

# App Configuration
APP_NAME=My React Native App
APP_VERSION=1.0.0
BUILD_NUMBER=1